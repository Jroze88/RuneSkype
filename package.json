{
  "name": "runeskype",
  "version": "1.0.0",
  "description": "Nerd Video Chat",
  "engines": {
    "node": "8.11.2",
    "npm": "6.4.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Jano Roze, Tamara Hartman, Sulai Sivadel",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chance": "^1.0.16",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "yarn": "^1.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jroze88/RuneSkype.git"
  },
  "bugs": {
    "url": "https://github.com/Jroze88/RuneSkype/issues"
  },
  "homepage": "https://github.com/Jroze88/RuneSkype#readme"
}
